<ul>
  <% polls.each(function (poll) { %>
    <li>
      <h1><%= poll.escape("title") %></h1>
       <a href="#/polls/<%= poll.id %>/edit" >edit</a>
       <a href="#/polls/<%= poll.id %>/email" >email</a>
       <a href="#/polls/<%= poll.id %>/vote" >vote</a>
       <a href="#/polls/<%= poll.id %>/results">results</a>
    </li>
  <% }); %>
</ul>